{
  "name": "juml",
  "displayName": "juml",
  "description": "juml is an extension for Visual Studio Code to generate UML diagram from source code.",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Visualization"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "juml.Source_to_Diagram",
        "title": "juml: Convert JAVA sources to Drawio diagram"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^18.11.9",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.6.4"
  },
  "extensionDependencies": [
    "hediet.vscode-drawio"
  ],
  "icon": "images/icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/owpk/juml"
  },
  "publisher": "owpk",
  "dependencies": {
    "bowser": "^2.11.0",
    "os-name": "^5.0.1",
    "window": "^1.0.0"
  }
}
